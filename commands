# insert commands 
CONNECT iti
INSERT INTO students VALUES ( 5, mina, 10 )   


# delete commands
CONNECT iti
DELETE FROM students WHERE id = 15


# update commands
CONNECT iti
UPDATE students SET degree=100 WHERE name=mina

# create commands
CONNECT iti
CREATE TABLE staff ( id int pk, name string, salary int )

# list all
LIST ALL

# exit
EXIT